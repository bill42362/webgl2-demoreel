{
  "name": "webgl2-demoreel",
  "version": "1.0.0",
  "description": "Tech demo of WebGL2.",
  "main": "dist/server/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bill42362/webgl2-demoreel"
  },
  "license": "MIT",
  "scripts": {
    "clidocs": "jsdoc2md -t template/*.hbs --partial template/partial/*.hbs --helper template/helper.js -f src/client/js/**/*.js",
    "docs": "yarn -s clidocs > README.md",
    "devdocs": "nodemon --exec 'yarn clidocs' -e js,jsx,hbs --watch template --watch src/client/js",
    "wiki": "babel-node template/wiki.js",
    "devwiki": "nodemon --exec 'yarn wiki' -e js,jsx,hbs --watch template --watch src/client/js",
    "prettier": "prettier -l --write '**/*.js' '**/*.jsx' '**/*.json' --ignore-path .gitignore",
    "clean": "rimraf dist",
    "buildclient": "webpack --progress --config webpack/client.babel.js",
    "buildserver": "webpack --progress --config webpack/server.babel.js",
    "buildhtml": "babel-node src/server/buildHtml.js",
    "build": "yarn clean && yarn buildclient && yarn buildserver && yarn buildhtml",
    "cloudbuild": "webpack --config webpack/client.babel.js && yarn buildhtml",
    "analyse": "BUNDLE_ANALYSE=true yarn build",
    "start": "yarn build && node dist/server",
    "devstart": "babel-node --max_old_space_size=8192 src/server",
    "devtest": "jest --watch --config jest/jest.config.js",
    "prePush": "sh prePush.sh",
    "test": "jest --coverage --config jest/jest.config.js"
  },
  "homepage": "https://github.com/bill42362",
  "dependencies": {
